    <Authors>Mark Embling</Authors>
    <Company>Mark Embling</Company>
    <Description>Library for interacting with the excellent Postcodes.io service.</Description>
    <Copyright>Copyright © Mark Embling and Contributors 2015-2020</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/markembling/MarkEmbling.PostcodesIO</PackageProjectUrl>
    <PackageIconUrl>https://static.markembling.info/nuget/icons/MarkEmbling.PostcodesIO.png</PackageIconUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.0.8</Version>
    <AssemblyVersion>0.0.8</AssemblyVersion>
    <FileVersion>0.0.8</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <PackageReference Include="RestSharp" Version="106.6.9" />
  </ItemGroup>

</Project>
